{
  "name": "vite-vue3-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test": "vite --mode test",
    "build": "vue-tsc --noEmit && vite build",
    "build:test": "vue-tsc --noEmit && vite build --mode test --outDir dist_test",
    "build:master": "vue-tsc --noEmit && vite build --mode master --outDir dist_master",
    "preview": "vite preview"
  },
  "dependencies": {
    "ant-design-vue": "^3.1.1",
    "axios": "^0.26.1",
    "echarts": "^5.3.2",
    "js-base64": "^3.7.2",
    "pinia": "^2.0.13",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.0",
    "less": "^4.1.2",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.19.2",
    "vite": "^2.9.0",
    "vue-tsc": "^0.29.8"
  }
}
